{
  "rewrites": [
    {
      "source": "/:path*",
      "destination": "http://vps-4623286-x.dattaweb.com:3000/:path*"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Content-Security-Policy",
          "value": "default-src 'self'; img-src 'self' data: https:;"
        }
      ]
    }
  ]
}
